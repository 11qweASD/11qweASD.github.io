    <entry>
      <title>都感觉哦视频分配配电房识破额哦该25ff6</title>
      <link href="/smk/hkkom/"/>
      <url>/smk/hkkom/</url>
      
        <content type="html"><![CDATA[<div class="video-container" id="videoContainer"></div><style>.video-container {  position: relative;  width: 100%;  padding-top: 56.25%; /* 16:9 */  background: #000;}.video-container iframe,.video-container video {  position: absolute;  top: 0; left: 0;  width: 100%;  height: 100%;  border: none;}</style><script>function embedVideo(containerId, videoUrl) {  const container = document.getElementById(containerId);  container.innerHTML = '';  if (!videoUrl) {    container.innerHTML = '<p style="color:#fff;text-align:center;">无有效视频链接</p>';    return;  }  const youtubeMatch = videoUrl.match(/(?:youtu\.be\/|youtube\.com\/watch\?v=|youtube\.com\/embed\/)([a-zA-Z0-9_-]+)/);  let element;  if (youtubeMatch) {    const videoId = youtubeMatch[1];    element = document.createElement('iframe');    element.src = `https://www.youtube.com/embed/${videoId}`;    element.allow = 'accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share';    element.allowFullscreen = true;    element.title = 'YouTube video player';    element.referrerPolicy = 'strict-origin-when-cross-origin';  } else if (videoUrl.match(/\.(mp4|webm|ogg)(\?.*)?$/i)) {    element = document.createElement('video');    element.src = videoUrl;    element.controls = true;    element.playsInline = true;    element.preload = 'metadata';  } else {    container.innerHTML = '<p style="color:#fff;text-align:center;">不支持的视频链接格式</p>';    return;  }  container.appendChild(element);}// 修改这里的视频链接即可const videoUrl = 'https://zb.ccq3.cn/%E5%9B%BE%E7%89%87/%E5%A5%BD%E7%9C%8B%E5%90%97.mp4';embedVideo('videoContainer', videoUrl);</script>]]></content>
      
      
